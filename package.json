{
  "name": "rn-dev-toolbox",
  "version": "0.2.2",
  "scripts": {
    "test": "jest",
    "flow": "flow",
    "standard": "standard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mbret/rn-dev-toolbox"
  },
  "dependencies": {
    "hoist-non-react-statics": "3.2.0",
    "memoize-one": "4.0.3"
  },
  "peerDependencies": {
    "react-native": "0.56.x | 0.57.x"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "coveralls": "3.0.2",
    "eslint-plugin-flowtype": "3.2.0",
    "flow-bin": "0.87.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.50.0",
    "react": "16.6.3",
    "react-native": "0.57.7",
    "react-test-renderer": "16.6.3",
    "standard": "12.0.1"
  },
  "main": "src/index.js",
  "author": "Maxime Bret",
  "license": "MIT",
  "description": "",
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "/flow-typed"
    ],
    "globals": [
      "__DEV__"
    ]
  },
  "jest": {
    "verbose": true,
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTests.js",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "lines": 60,
        "statements": 60,
        "functions": 69
      }
    }
  }
}
